<h1>Game started </h1>


<%= link_to "rest", match_path(@match) %> 

<div class="row top-buffer">
  <div class="col-md-6">
    <button type="submit" class="btn btn-default btn-lg btn-block">
      Serve Left
    </button>
  </div>

  <div class="col-md-6">
    <button type="submit" class="btn btn-default btn-lg btn-block">
      Serve Right
    </button>
  </div>
</div>

<div class="row top-buffer">
  <div class="col-md-6">
    <h3 class="text-info">Player 1 score: </h3>
    <button type="submit" class="btn btn-primary btn-lg btn-block">
      Point for Player 1
    </button>
  </div>

  <div class="col-md-6">
    <h3 class="text-info">Player 2 score: </h3>
    <button type="submit" class="btn btn-success btn-lg btn-block">
      Point for Player 2
    </button>
  </div>
</div>
<br>
<table class="table table-bordered table_style">
  <thead>
    <tr>
      <th class="col-md-1">Rally</th>
      <th class="col-md-1">Time</th>
      <th class="col-md-4">Player 1: Score</th>
      <th class="col-md-6">Player 2: Score</th>
    </tr>
  </thead>
  <tbody>
  <tr>
    <td class="col-md-1">Ral Num</td>
    <td class="col-md-1">Time</td>
    <td class="col-md-4">Player1 score</td>
    <td class="col-md-6">Player2 score</td>
  </tr>
  </tbody>
</table>

    

    
      
